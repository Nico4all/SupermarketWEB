@page
@model SupermarketWEB.Pages.Details.IndexModel
@{
    ViewData["Title"] = "Index Details";
}
<h1>Index Details</h1>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Details[0].Id)</th>
            <th>@Html.DisplayNameFor(model => model.Details[0].Quiantity)</th>
            <th>@Html.DisplayNameFor(model => model.Details[0].Price)</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Details)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.Quiantity)</td>
                <td>@Html.DisplayFor(modelItem => item.Price)</td>
                <td>
                    <span></span>
                </td>
            </tr>
        }
    </tbody>
</table>
